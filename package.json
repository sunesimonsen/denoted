{
  "type": "module",
  "name": "denoted",
  "private": true,
  "version": "1.0.0",
  "description": "Dropbox frontend for Emacs Denote files",
  "keywords": [
    "emacs",
    "denote"
  ],
  "author": "Sune Simonsen",
  "license": "MIT",
  "scripts": {
    "start": "vite --port 5000 public",
    "build": "rm -rf dist && vite build -c ./vite.config.js --outDir ../dist public && cp dist/index.html dist/200.html",
    "format": "prettier --write '**/*.{js,mjs,md,json}'; npm run lint:eslint -- --fix",
    "lint:format": "prettier --check '**/*.{js,mjs,md,json}'",
    "lint:eslint": "eslint '**/*.{js,mjs}'",
    "lint": "npm run lint:eslint && npm run lint:format",
    "deploy": "npm run build && surge dist denoted.surge.sh",
    "test": "mocha"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "babel-plugin-stylewars": "^0.1.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-mocha": "^10.5.0",
    "fake-promise": "^2.5.44",
    "globals": "^15.11.0",
    "mocha": "^10.2.0",
    "prettier": "^3.2.5",
    "surge": "^0.23.1",
    "unexpected": "^13.0.0",
    "unexpected-dependable": "^1.1.0",
    "vite": "^5.0.12"
  },
  "dependencies": {
    "@codemirror/commands": "^6.3.3",
    "@codemirror/lang-markdown": "^6.3.2",
    "@codemirror/language": "^6.10.1",
    "@codemirror/language-data": "^6.5.1",
    "@codemirror/state": "^6.4.0",
    "@codemirror/view": "^6.23.1",
    "@dependable/cache": "^0.11.0",
    "@dependable/components": "^0.45.0",
    "@dependable/debounce": "^0.1.1",
    "@dependable/icons": "^0.6.0",
    "@dependable/nano-router": "^0.9.0",
    "@dependable/state": "^0.20.0",
    "@dependable/view": "^0.22.0",
    "@dependable/vite": "^0.1.0",
    "@lezer/highlight": "^1.2.1",
    "@nano-router/history": "^2.12.0",
    "@nano-router/router": "^2.12.0",
    "marked": "^15.0.7",
    "marked-linkify-it": "^3.1.12",
    "stylewars": "^2.4.0"
  }
}
